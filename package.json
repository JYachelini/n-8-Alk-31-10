{
  "name": "aconcagua-backend",
  "version": "1.0.1",
  "description": "Earn time with this powerful, scalable and easy to config boilerplate.",
  "author": "gabodmc",
  "license": "MIT",
  "homepage": "https://github.com/gabodmc/nodejs-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabodmc/nodejs-boilerplate"
  },
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand --forceExit --detectOpenHandles",
    "dev": "nodemon .",
    "prepare": "husky install",
    "seed": "sequelize-cli db:seed:all",
    "migrate": "sequelize-cli db:migrate"
  },
  "engines": {
    "node": ">=0.14"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.13.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.19.0",
    "sequelize-cli": "6.2.0",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "jest": "27.0.6",
    "lint-staged": "^13.0.3",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.20",
    "prettier": "2.3.2",
    "supertest": "^6.1.6"
  },
  "lint-staged": {
    "./**/*.js": [
      "prettier --write",
      "eslint"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
